<script id="msg-template" type="text/x-handlebars-template">
  <div class="popup-box chat-popup" id="qnimate">
  <div class="popup-head">
  <div class="popup-head-left pull-left">
	  
	  \{{#data.dp}}
                  <img src="data:image/png;base64, \{{data.dp}}"/>

    \{{/data.dp}}

    \{{^data.dp}}
                   <img src="/profileAssets/images/male.jpg"/>
                  
    \{{/data.dp}}
	  
	   \{{data.FirstName}} \{{data.LastName}} \{{data.CompanyName}}
     
     </div>
  <div class="popup-head-right pull-right">
  <div class="btn-group">
  <button class="chat-header-button" data-toggle="dropdown" type="button" aria-expanded="false">
  <i class="glyphicon glyphicon-cog"></i> </button>
  <ul role="menu" class="dropdown-menu pull-right">
  <li><a href="#">Media</a></li>
  <li><a href="#">Block</a></li>
  <li><a href="#">Clear Chat</a></li>
  </ul>
  </div>
						
  <button  id="closeChatBox" onclick="" class="chat-header-button pull-right" type="button"><i class="glyphicon glyphicon-off"></i></button>
  </div>
  </div>
 
  <div class="popup-messages" id="popupMsg">
     <div class="chat-box-single-line">
  <abbr class="timestamp">January 6th, 2020</abbr>
 <div class="direct-chat-messages" id="i" >
<div class="direct-chat-msg doted-border" id="displayMessage">


</div>

 </div>
  
  </div>
 
  </div>
  <div class="popup-messages-footer">
    <form id="msgForm">
  <input name="msgText" type="text" autocomplete="off" placeholder="Type your message here..."style="1px solid #b0b0b0" class="form-control">
  </form>
  <div class="btn-footer">
  <button class="bg_none"><i class="glyphicon glyphicon-film"></i> </button>
  <button class="bg_none"><i class="glyphicon glyphicon-camera"></i> </button>
  <button class="bg_none" id="att"><i class="glyphicon glyphicon-paperclip"></i> </button>
  <button class="bg_none pull-right"><i class="glyphicon glyphicon-thumbs-up"></i> </button>
  </div>
  </div>
  </div>
  </script>



<script id="msg" type="text/x-handlebars-template">
  
  \{{#data}}
  <div class="direct-chat-info clearfix">
   \{{#u1}}
  <span class="direct-chat-name pull-left">\{{user1.FirstName}} \{{user1.LastName}} \{{user1.CompanyName}}</span>
   \{{/u1}}

   \{{#u2}}
      <span class="direct-chat-name pull-left">\{{user2.FirstName}} \{{user2.LastName}} \{{user2.CompanyName}}</span>
   \{{/u2}}

   \{{^u1}}
     \{{^u2}}
      <span class="direct-chat-name pull-left">\{{FirstName}} \{{LastName}} \{{CompanyName}}</span>
     \{{/u2}}
   \{{/u1}}

  </div>
      
      \{{#u1}}
       \{{#user1.dp}}
      <img src="data:image/png;base64, \{{user1.dp}} " id="i" alt="user" name="user" class="direct-chat-img" />
        \{{/user1.dp}}
        \{{^user1.dp}}
        <img src="/profileAssets/images/male.jpg" class="direct-chat-img"/>
        \{{/user1.dp}}
      \{{/u1}}
       
      \{{#u2}}
       \{{#user2.dp}}
        <img src="data:image/png;base64, \{{user2.dp}} " id="i" alt="user" name="user" class="direct-chat-img" />
       \{{/user2.dp}}
       \{{^user2.dp}}
        <img src="/profileAssets/images/male.jpg" class="direct-chat-img"/>
       \{{/user2.dp}}
      \{{/u2}}
      
     \{{^u1}}
      \{{^u2}}
       \{{#dp}}
        <img src="data:image/png;base64, \{{dp}} " id="i" alt="user" name="user" class="direct-chat-img" />
       \{{/dp}}
       \{{^dp}}
        <img src="/profileAssets/images/male.jpg" class="direct-chat-img"/>
       \{{/dp}}
      \{{/u2}}
     \{{/u1}}


      \{{#Message}}
  <div class="direct-chat-text">
  \{{Message}}
  
  </div>
  \{{/Message}}
  \{{#Attachment}}
  <div class="direct-chat-text" style="padding: 0px;">
    \{{#Type.image}}
  <img src="data:image;base64, \{{Attachment}}" style="width:209px"/>
  \{{/Type.image}}
  \{{#Type.pdf}}
  
   <object style="padding:0px;" width="209px" height="128.8px" data="data:application/pdf;base64, \{{Attachment}}">
    
  </object>
  \{{/Type.pdf}}
    \{{#Type.txt}}
  
   <object style="padding:0px;" width="209px" height="128.8px" data="data:text/plain;base64, \{{Attachment}}">
    
  </object>
  \{{/Type.txt}}
  
  </div>
  \{{/Attachment}}
  <div class="direct-chat-info clearfix">
  <span class="direct-chat-timestamp pull-right">\{{time}}</span>
  </div>
 
\{{/data}}
\{{#error}}
  <div class="direct-chat-info clearfix">
    <div class="alert alert-warning" role="alert">
     \{{error}}
  </div>
    
  </div>
  \{{/error}}
  

</script>


