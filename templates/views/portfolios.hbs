<script id="portfolio-thumbnail" type="text/x-handlebars-template">
         \{{#Portfolios}}
          <div class="col-md-4 portfolio">
                    <div class="box">
                        <img src="/project-portfolio/images/portfolio2.jpg">
                        <h3 class="title">\{{Title}}</h3>
                        <div class="box-content">
                    
                           <a href="javascript:openPortfolio('\{{_id}}',
                           '\{{total}}','\{{average}}',
                           '\{{rating.1}}', '\{{rating.2}}', '\{{rating.3}}', '\{{rating.4}}',
                            '\{{rating.5}}'
                           )"> <i class="fa fa-folder-open-o fa-4x icon-set " aria-hidden="true"></i></a> 
                        </div>
                    </div>
                    <input class="rating rating-loading rated" value="\{{average}}" data-min="0" data-max="5" data-step="0.5" data-size="lg">
                    <span style="float: right;"><span class="glyphicon glyphicon-user"></span>\{{total}} total ratings</span>
                </div> 
          \{{/Portfolios}}
    </script>
