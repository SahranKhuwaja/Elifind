	    <div class="card-body">
	        <div class="row">
        	    <div class="col-md-2">
        	        \{{#Dp}}
        	        <img src="data:image/png;base64, \{{Dp}}" class="img img-rounded img-fluid img-r"/>
                  \{{/Dp}}
        	        \{{^Dp}}
                   <img src="/profileAssets/images/male.jpg" class="img img-rounded img-fluid img-r"/>
                  \{{/Dp}}
        	    </div>
        	    <div class="col-md-10">
        	        <p class="review">
        	            <a class="float-left" href="/Profile/View/\{{_id}}/Timeline"><strong>
                        \{{FirstName}} \{{LastName}} \{{CompanyName}}
                      </strong></a>
                      <span class="set-role-review">\{{Role}}</span>
        	            <input class="rating rating-loading ratedForReview" value="\{{userRating}}" data-min="0" data-max="5" data-step="0.1" data-size="sm">
        	            <span >\{{time}}</span>
                 </p>
        	       <div class="clearfix"></div>
        	        <p class="review ">\{{Review}}</p>
        	        <p class="set-reviews-bottom-icons">
        	            <a class="float-right btn btn-outline-primary ml-2 btn-bottom-review"> <i class="fa fa-reply"></i> Reply</a>
        	            <a class="float-right btn  btn-bottom-review "> <i class="fa fa-heart"></i> Like</a>
        	       </p>
        	    </div>
	        </div>
	    </div>
	</div>
     \{{/data}}
  \{{#reviewed}}
  <div class="alert alert-success sc-r" role="alert">
         Success! Your review has been recorded successfully!
  </div>
  \{{/reviewed}}
  \{{^reviewed}}
   <div class="card reviews" id="myCommentArea">
	    <div class="card-body">
	        <div class="row">
        	    <div class="col-md-2">
        	       {{#if Pimage}}
        	           <img src="data:image/png;base64, {{Pimage}}" class="img img-rounded img-fluid img-r"/>
                  {{else}}
                     <img src="/profileAssets/images/male.jpg" class="img img-rounded img-fluid img-r"/>
                  {{/if}}
        	    </div>
        	    <div class="col-md-10">
        	        <p class="review">
                      {{#if user}}
        	            <a class="float-left" href="/Profile/View/{{user._id}}/Timeline"><strong>{{user.FirstName}} {{user.LastName}}</strong></a>
                      {{else if loggedInUser.CompanyName}}
                      <a class="float-left" href="/Profile/View/{{loggedInUser._id}}/Timeline"><strong>{{loggedInUser.CompanyName}} </strong></a>
                      {{else}}
                        <a class="float-left" href="/Profile/View/{{loggedInUser._id}}/Timeline"><strong>{{loggedInUser.FirstName}} {{loggedInUser.LastName}} </strong></a>
                      {{/if}}
        	            <input class="rating rating-loading ratedForReview" id="myRating" value="\{{rating}}" data-min="0" data-max="5" data-step="0.1" data-size="sm">
        	            <span ></span>
                 </p>
        	       <div class="clearfix"></div>
                 <div class="review">
        	       <textarea class="review-textArea" id="comment" rows="6" placeholder="Write your reviews"></textarea>
                  <button class="btn btn-primary btn-review" id="btnReview">Submit</button>
        	        </div>
                 
        	    </div>
	        </div>
	    </div>
	</div>
  \{{/reviewed}}
  </script>

