              </ul>
            </div>



          




            <div class="col-md-7">

              {{#if user}}
              <div class="edit-profile-container" id="info">
                <div class="block-title">
                  <h4 class="grey"><i class="ion-ios-information-outline icon-in-title"></i>Basic Information</h4>
                  
                   <a href="" style="margin-left: 560px" id="edit">Edit Profile</a>
                 
                  <div class="line"></div>
                </div>
                 
                <div class="edit-block" style="margin-top: 10px" id="editProfileDetails">
                  <form name="basic-info" id="basic-info" class="form-inline" method="POST" action="/UpdateInfo">
                    <div class="row">
                      <div class="form-group col-xs-6">
                        <label for="firstname">First name</label>
                        <input id="fname" class="form-control input-group-lg cursor" type="text" name="FirstName" title="Enter first name" placeholder="First name" value="{{user.FirstName}}" disabled/>
                      </div>
                      <div class="form-group col-xs-6">
                        <label for="lastname" class="">Last name</label>
                        <input id="lname" class="form-control input-group-lg cursor" type="text" name="LastName" title="Enter last name" placeholder="Last name" value="{{user.LastName}}" disabled/>
                      </div>
                    </div>
                    <div class="row">
                      <div class="form-group col-xs-12">
                        <label for="email">My email</label>
                        <input id="email" class="form-control input-group-lg cursor" type="text" name="Email" title="Enter Email" placeholder="My Email" value="{{user.Email}}" disabled/>
                      </div>
                    </div>
                    <div class="row">
                     
                      <div class="form-group col-xs-12">
                          <label for="Bithday">Date of birth</label>
                            <input id="Birthday" class="form-control input-group-lg" type="date" name="Birthday" title="" placeholder="" value="{{dpt}}" style="display: none"/>
                        <input id="dob" class="form-control input-group-lg cursor" type="text" name="" title="" placeholder="" value="{{dob}}" disabled/>
                       
                      </div>
                    </div>
                    <div class="row">
                   <div class="form-group col-xs-6">
                        <label for="country">Gender</label>
                        <input type="text" id="g" value="{{user.Gender}}"hidden>
                        <select class="form-control cursor" id="Gender" name="Gender" disabled>
                          <option value="Male">Male</option>
                          <option value="Female">Female</option>
                          <option value="Other">Other</option>
                                                  
                        </select>
                      </div>
                    </div>
                    <div class="row">
                      <div class="form-group col-xs-6">
                        <label for="city"> My city</label>
                        <input id="city" class="form-control input-group-lg cursor" type="text" name="City" title="Enter city" placeholder="Your city" value="{{user.City}}" disabled/>
                      </div>
                      <div class="form-group col-xs-6">
                        <label for="country">My country</label>
                        <select class="form-control cursor" id="country" name="Country"disabled>
                          <option value="country" disabled>Country</option>
                          <option value="{{user.Country}}" id="c" hidden>{{user.Country}}</option>
                          {{#each CountryData}}
                          <option value="{{this.name}}">{{this.name}}</option>
                          {{/each}}                          
                        </select>
                      </div>
                    </div>
                    <div class="row">
                      <div class="form-group col-xs-12">
                        <label for="my-info">Phone</label>
                        <input type="text" id="Phone" name="Phone" value="{{user.Phone}}" class="form-control cursor" placeholder="Phone" name="phone" disabled />
                      </div>
                    </div>
                    <button class="btn btn-primary" type="submit" id="save" style="display: none">Save Changes</button>
                  </form>

                  
                </div>
              </div>
                     <div class="edit-profile-container" id="skillSection">
                <div class="block-title">
                  <h4 class="grey"><i class="icon ion-ios-heart-outline"></i> Skills </h4>
                  <div class="line"></div>
                  <p> These are my Skills.</p>
                  <div class="line"></div>
                </div>
                <div class="edit-block">
                  <ul class="list-inline interests">
                    {{#each skills}}
                  	<li><a href="#"><i class="icon ion-ios-lightbulb"></i> {{Skill}}</a></li>
                     {{/each}}
                  </ul>
                  <div class="line"></div>
                  {{#if skills}}
