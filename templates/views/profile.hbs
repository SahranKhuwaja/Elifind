                    <p class="custom-label"><strong>Add Skill</strong></p>
                     <form method="POST" action="/Profile/View/Inomash/Update">
                    <div class="form-group col-sm-8">
                      <input id="" class="form-control input-group-lg" type="text" name="Skill" placeholder="Skills. For example, Android, node js etc" required/>
                    </div>
                    <div class="form-group col-sm-8">
                      <select type="text" name="Category" id="Category" class="form-control input-group-lg" placeholder="Category" onchange="cat(this)">
                                        <option value="" disabled>Select Category</option>
                                        <option value="Web Development (Software)">
                                          Web Development (Software)
                                        </option>
                                        <option value="Desktop Application (Software)">
                                          Desktop Application (Software)
                                        </option>
                                        <option value="Artificial Intelligence (Software/Hardware)">
                                          Artificial Intelligence (Software/Hardware)
                                        </option>
                                        <option value="Robotics (Software/Hardware)">
                                          Robotics (Software/Hardware)
                                        </option>
                                        <option value="Electronics (Hardware)" >
                                          Electronics (Hardware)
                                        </option>
                                        <option value="IOT (Software/Hardware)">
                                          IOT (Software/Hardware)
                                        </option>
                                        <option value="AnyOtherSoftware (Software)">
                                          Any Other Software (Software)
                                        </option>
                                        <option value="AnyOtherHardware">
                                          Any Other Hardware (Hardware)
                                        </option>
                                        <option value="">
                                        Other
                                        </option>
                                    </select>
                    </div>
                     <div class="form-group col-sm-8">
                      <input class="form-control input-group-lg" type="text" id="Other" name="Category" placeholder="Other" style="pointer-events:none" disabled required/>
                    </div>
                    <div class="form-group col-sm-8">
                     <select type="text" name="GoldenSkill" id="gs" class="form-control input-group-lg" placeholder="Golden Skills">
                                        <option value="true">
                                          Yes
                                        </option>
                                        <option value="false" selected>
                                          No
                                        </option>
                     </select>
                    </div>
                    <div class="form-group col-sm-8">
                            <input type="number" name="SkillRate" class="form-control input-group-lg" value="50" min="50" max="100">
                      
                    </div>
                    <div class="form-group col-sm-4">
                      <button class="btn btn-primary" type="submit">Add</button>
                    </div>
                    </form>
                      
            </div>
            {{/if}}
            <div class="alert alert-success" style="margin-top:14px;" id="skillM" role="alert" hidden>
              	<strong><p id="success-add-skill" style="font-size:15px;" hidden>{{successSS}}</p></strong>
                </div>
                  </div>
                  
              </div>
