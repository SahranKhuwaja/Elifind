<body style="overflow-y:hidden ;" id="whole">


<div class="sahran_khuwaja_wrapper_all">

	<div id="sahran_khuwaja_popup_blog">
		<div class="container">
			<div class="inner_popup scrollable"></div>
		</div>
		<span class="close"><a href="#"></a></span>
	</div>
	

	<div class="sahran_khuwaja_preloader">
		<div class="spinner_wrap">
			<div class="spinner"></div>
		</div>
	</div>


	<form id="f1">
   
	<div class="sahran_khuwaja_content">
		
				<div class="sahran_khuwaja_section" id="home">
					<div class="sahran_khuwaja_hero_header_wrap">
						<div class="sahran_khuwaja_universal_box_wrap particle jarallax" data-speed="0.1">
							<div id="particles-js"></div> 
							<div class="particle_overlay"></div>
							<div class="inner_content">
								<div class="image_wrap">
									{{#if image}}
									<img src="data:image/png;base64, {{image}}" alt="" />

                                     {{else}}
                                       
									   <img src="/profileAssets/images/male.jpg" alt="" />

									{{/if}}
								</div>
								<div class="name_holder">
								<h3>{{userInfo.FirstName}} <span>{{userInfo.LastName}} </span></h3>
								</div>
								<div class="text_typing">
									<p id = "para">I'm a <span class="sahran_khuwaja_animation_text_word"></span></p>
								</div>
							</div>
							<div class="sahran_khuwaja_arrow_wrap bounce anchor">
								<a href="#inomash"><i class="xcon-angle-double-down"></i></a>
							</div>
						</div>
			

	
	

		<section id="" class="main-block" style="background:ivory;padding-top:800px">
			<div class="container text-center" id="inomash">
			
				
				<div class="main-title text-left wow fadeIn">
					<div class="main-title__inner">
						<div class="text-right main-title__left">
							<i class="fa fa-female mega"></i>
							<h1 class="text-uppercase heavy lh padder">profile</h1>
							<em class="h5 serif em text-capitalize regular">my personal info</em>
						</div>
						<div class="main-title__right">
							<p class="serif em regular h6">
								This section contains my personal infomation. It reflects my personality. 
                                In todayâ€™s digital economy, personal information is the new currency.
							</p>
						</div>
					</div>
				</div>

			
				<div class="row main-block__padder text-left">
					
					
					<div class="col-lg-4 col-md-6 col-lg-push-4 text-center wow fadeInUp">
						<div class="inline-block photo-column-inner">
							<div class="image_wrap center-block">
								{{#if image}}
									<img src="data:image/png;base64, {{image}}"  class="img-responsive" alt="" />

                                     {{else}}
                                       
									   <img src="/profileAssets/images/male.jpg" class="img-responsive" alt="" />

									{{/if}}
							</div>
							{{#if loginUserInomash}}
							<div class="md-padder">
								
								<button class="btn btn-block btn--main-color" id="btnD" onclick="download();">Download InoMash</button>
							</div>
							<div class="md-padder">
								
								<button class="btn btn-block btn--main-color" id="btnD2" onclick="window.print()">Print InoMash</button>
							</div>
							{{/if}}
						</div>
					</div>

					
					<div class="col-lg-4 col-md-6 col-lg-push-4 info-column wow fadeInUp">
						<div>
							<h5 class="text-capitalize">personal info</h5>
							<ul class="padder list-unstyled info-list">
								<li>Name : {{userInfo.FirstName}} {{userInfo.LastName}}</li>
								<li>Age : {{age}} Years Old</li>
								<li>Gender : {{userInfo.Gender}}</li>
								<li>Phone : {{#if userInfo.Phone}}

									     {{userInfo.Phone}} 

										 {{else}}
										 &nbsp &nbsp &nbsp &nbsp-
									{{/if}}
								</li>
								<li>Email : <a href="mailto:{{userInfo.Email}}" style="color:cornflowerblue;">{{userInfo.Email}}</a></li>
								<li>City : {{userInfo.City}}</li>
								<li>Country : {{userInfo.Country}}</li>
								
							</ul>
							<ul class="list-inline list-gutter-xs padder h5 bare-social-menu">
								<li>
									<a href="#" class="twitter-color">
										<i class="fa fa-twitter"></i>
									</a>
								</li>
								<li>
									<a href="#" class="google-plus-color">
										<i class="fa fa-google-plus"></i>
									</a>
								</li>
								<li>
									<a href="#" class="behance-color">
										<i class="fa fa-behance"></i>
									</a>
								</li>
								<li>
									<a href="#" class="dribbble-color">
										<i class="fa fa-dribbble"></i>
									</a>
								</li>
								<li>
									<a href="#" class="github-color">
										<i class="fa fa-github"></i>
									</a>
								</li>
							</ul>
						</div>
					</div>	

					
					<div class="col-lg-4 col-lg-pull-8 col-xs-12 text-left story-column wow fadeInUp">
						<div>
							<h5 class="text-capitalize">My summary</h5>
							<p class="padder">
							  To be continued
							</p>
							<p class="padder">
								
							</p>
							<div class="lg-padder">
								<br><br><br><br><br><br><br><br><br><br><br><br>
								<img src="/inomashViewAssets/img/signature.png" alt="#">
							</div>
						</div>
					</div>

				</div>

			</div>
		</section>
		
							
						

							

		<section id="prof" class="main-block" style="background:ivory">
			<div class="container text-center" id="p">
				
				
				<div class="main-title text-left wow fadeIn">
					<div class="main-title__inner">
						<div class="text-right main-title__left">
							<i class="fa fa-book mega"></i>
							<h1 class="text-uppercase heavy lh padder">Professional Details</h1>
							<em class="h5 serif em text-capitalize regular">know who i'm</em>
						</div>
						<div class="main-title__right">
							<p class="serif em regular h6">
								This section contains my acedemic and professional details, which will help me building an amzaing career. Although my InoMash depends on my skills, but acedemic and professional details also plays a vital role.
							</p>
						</div>
					</div>
				</div>

				
				<div class="main-block__padder text-left">
					
					
					<div class="table wow fadeIn">
						<div class="table-cell h5 secondary-title__icon">
							<i class="fa fa-graduation-cap"></i>
						</div>
						<div class="table-cell secondary-title__text">
							<h5 class="text-capitalize">Education</h5>
						</div>
						<div class="table-cell secondary-title__line"></div>
					</div>

				
					<div class="main-block__sub-padder">

						{{#if user.Courses}}
						{{#each user.Courses}}
						<div class="timeline education-timeline">
							
							
							<div class="timeline-rail">
								<span class="timeline-loader-btn plus-icon plus-icon-small btn--main-color"></span>
							</div>
							
							
							<div class="timeline-items list-gutter-lg">
								
							
								<div class="clearfix timeline-item">
									
									<div class="pull-left clearfix text-center">
										
										
										<div class="inline-block v-middle">
													
											
											<div class="custom-tooltip bold text-uppercase small inline-block v-middle hidden-xs wow fadeInLeft">
												<span>
													<i class="fa fa-clock-o normal"></i>
												</span>
												{{this.sy}} - {{this.ey}}
											</div>
											<div class="timeline-item-bullet inline-block v-middle"></div>

										</div>
										<div class="timeline-box inline-block v-middle hidden-xs text-uppercase wow fadeInRight">
											<span class="h3 light">A+</span>
											<small class="bold">grade</small>
										</div>

									</div>

									
									<div class="no-overflow timeline-item-info wow fadeInRight">
										<h6 class="text-capitalize lh">{{this.CourseTitle}}</h6>
										<ul class="list-inline list-gutter-xs em serif text-capitalize sm-padder timeline-info-list">
											<li>{{this.Institute}}</li>
											<li>&#8226;</li>
											<li>{{userInfo.Country}}</li>
										</ul>
										<p class="padder">
										 This is one of my degree. It reflects my acedemic detail. Although it is not important for my career. Because Elifind provides me the opportunity to flourish without degree. Skills are import for my career. 
										</p>
									</div>

								</div>
								
								
							</div>	

						</div>
						{{/each}}
						{{else}}
						<p class="padder">I am a self learner. </p>
						{{/if}}

					</div>

				</div>

				
				<div class="main-block__padder text-left">
					
					
					<div class="table wow fadeIn">
						<div class="table-cell h5 secondary-title__icon">
							<i class="fa fa-flask"></i>
						</div>
						<div class="table-cell secondary-title__text">
							<h5 class="text-capitalize">experience</h5>
						</div>
						<div class="table-cell secondary-title__line"></div>
					</div>

					
					<div class="main-block__sub-padder">
						{{#if user.WorkExperience}}
						{{#each user.WorkExperience}}
						<div class="timeline">
							
							
							<div class="timeline-rail">
								<span class="timeline-loader-btn plus-icon plus-icon-small btn--main-color"></span>
							</div>
							
							
							<div class="timeline-items list-gutter-lg">
								
								
								
								<div class="clearfix timeline-item">
									
									
									<div class="pull-left clearfix text-center">
										
										
										<div class="inline-block v-middle">
													
											
											<div class="custom-tooltip bold text-uppercase small inline-block v-middle hidden-xs wow fadeInLeft">
												<span>
													<i class="fa fa-clock-o normal"></i>
												</span>
												{{this.sy}} - {{this.ey}}
											</div>
											<div class="timeline-item-bullet inline-block v-middle"></div>

										</div>
										<div class="timeline-box inline-block v-middle hidden-xs text-uppercase wow fadeInRight" style="background-image: url(/inomasgViewAssets/img/01.png)"></div>

									</div>

								
									<div class="no-overflow timeline-item-info wow fadeInRight">
										<h6 class="text-capitalize lh">{{this.ExperienceIn}}</h6>
										<ul class="list-inline list-gutter-xs em serif text-capitalize sm-padder timeline-info-list">
											<li>{{this.WorkPlace}}</li>
											<li>&#8226;</li>
											<li>{{userIno.Country}}</li>
										</ul>
										<p class="padder">
											This is one of my professional experience. Due experience has improved
										</p>
									</div>

								</div>
								
								
								

									
									

								</div>

							</div>
							{{/each}}
							{{else}}
								  <p class="padder">I am an amateur.</p>
								{{/if}}

						</div>
					</div>

				</div>

				

		<section id="skills" class="main-block">
			<div class="container text-center" id="skill">
				
				
				<div class="main-title text-left wow fadeIn">
					<div class="main-title__inner">
						<div class="text-right main-title__left">
							<i class="fa fa-pie-chart mega"></i>
							<h1 class="text-uppercase heavy lh padder">Skills</h1>
							<em class="h5 serif em text-capitalize regular">How good i'm</em>
						</div>
						<div class="main-title__right">
							<p class="serif em regular h6">
								Skills are very essential for my career. It refects my personality and capabilities. It really matters on this site. Because my entire career depends on my skills.
							</p>
						</div>
					</div>
				</div>

				
				<div class="main-block__padder text-left" style="padding-top: 0px;padding-bottom:200px">
				
					
					<div class="main-block__sub-padder">
						
						{{#each user.Skills}}
						<div class="timeline">
							
							
							<div class="timeline-rail">
								<span class="timeline-loader-btn plus-icon plus-icon-small btn--main-color"></span>
							</div>
							
							
							<div class="timeline-items list-gutter-lg">
								
								
								
								<div class="clearfix timeline-item">
									
									
									<div class="pull-left clearfix text-center">
										
										
										<div class="inline-block v-middle">
													
											
											<div class="custom-tooltip bold text-uppercase small inline-block v-middle hidden-xs wow fadeInLeft">
												<span>
													<i class="fa fa-lightbulb-o normal">
														
													</i>
												</span>
												{{this.gs}}
												
											</div>
											<div class="timeline-item-bullet inline-block v-middle"></div>

										</div>
										<div class="timeline-box inline-block v-middle hidden-xs text-uppercase wow fadeInRight" style="background-image: url(/inomasgViewAssets/img/01.png)"></div>

									</div>

								
									<div class="no-overflow timeline-item-info wow fadeInRight">
										<h6 class="text-capitalize lh">{{this.Skill}}</h6>
										<ul class="list-inline list-gutter-xs em serif text-capitalize sm-padder timeline-info-list">
											<li>{{this.Category}}</li>
											<li>&#8226;</li>
											<li>{{this.SkillRate}}%</li>
										</ul>
										<p class="padder">
											<div class="progress">
                                                  <div class="progress-bar progress-bar-striped bg-warning" role="progressbar" style="width: {{this.SkillRate}}%" aria-valuenow="{{this.SkillRate}}" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div> 
										</p>
									</div>

								</div>
								
								
								

									
									

								</div>

							</div>
							{{/each}}
						
								

						</div>
					</div>

				</div>

			</div>
